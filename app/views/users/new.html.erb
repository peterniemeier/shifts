<h2>Sign Up</h2>
<form class="" action="<%= users_url %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
  <input id="name" type="text" name="user[name]" value="" placeholder="Name">
  <input id="user_name" type="text" name="user[user_name]" value="" placeholder="User Name">
  <input id = "password" type="password" name="user[password]" value="" placeholder="Password">
  <input type="submit" name="" value="Sign Up">
</form>
