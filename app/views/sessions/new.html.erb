<!-- "sessions new" -->
<% unless logged_in?%>
  <h2>Sign In</h2>
  <form class="" action="<%= session_url %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
    <input id="user_name" type="text" name="user[user_name]" value="" placeholder="User Name">
    <input id = "password" type="password" name="user[password]" value="" placeholder="Password">
    <input type="submit" name="" value="Sign In">
  </form>
<% end %>
